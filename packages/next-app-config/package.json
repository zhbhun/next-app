{
  "name": "@next-app/config",
  "version": "0.0.1",
  "description": "Next config for app",
  "author": "zhbhun",
  "license": "MIT",
  "homepage": "https://github.com/zhbhun/next-mobile",
  "repository": "https://github.com/zhbhun/next-mobile/tree/master/packages/next-app-config",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib",
    "vendors"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && babel src --out-dir lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "css-loader": "1.0.0",
    "cssnano": "^4.1.7",
    "extracted-loader": "1.0.4",
    "find-up": "2.1.0",
    "ignore-loader": "0.1.2",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.4.3",
    "next": "https://github.com/zhbhun/next.js/releases/download/v7.0.3-beta.5/next-7.0.3-beta.5.tgz",
    "next-compose-plugins": "^2.1.1",
    "next-images": "^1.0.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "3.0.0",
    "sass-loader": "6.0.6",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "eslint-config-ease-base": "^2.0.0",
    "lint-staged": "^7.3.0",
    "prettier-config-ease": "^2.0.0",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6"
          },
          "spec": false,
          "loose": false,
          "modules": "commonjs",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        }
      ]
    ],
    "sourceType": "unambiguous"
  }
}
